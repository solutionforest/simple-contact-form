{
    "name": "solution-forest/simple-contact-form",
    "description": "This is my package simple-contact-form",
    "keywords": [
        "solutionforest",
        "laravel",
        "simple-contact-form"
    ],
    "homepage": "https://github.com/solutionforest/simple-contact-form",
    "support": {
        "issues": "https://github.com/solutionforest/simple-contact-form/issues",
        "source": "https://github.com/solutionforest/simple-contact-form"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "hayward",
            "email": "info@solutionforest.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/termwind": "^2.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "SolutionForest\\SimpleContactForm\\": "src/",
            "SolutionForest\\SimpleContactForm\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolutionForest\\SimpleContactForm\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SolutionForest\\SimpleContactForm\\SimpleContactFormServiceProvider"
            ],
            "aliases": {
                "SimpleContactForm": "SolutionForest\\SimpleContactForm\\Facades\\SimpleContactForm"
            }
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}